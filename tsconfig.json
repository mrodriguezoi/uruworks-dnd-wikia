{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "resolveJsonModule": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "esModuleInterop": true,
    "isolatedModules": true,
    "types": ["astro/client"]
  },
  "include": ["src/**/*", "*.d.ts", "src/pages/api/articles.ts"]
}
