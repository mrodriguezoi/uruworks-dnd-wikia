---
import SearchBar from './SearchBar.astro';
interface Props {
    title: string;
}
const { title } = Astro.props;
---
<div class="flex justify-between items-center mb-4 border-b border-grey">
    <header class="p-4">
        <h1 class="h1">{title}</h1>
    </header>
    <SearchBar> </SearchBar>
</div>
</div>

